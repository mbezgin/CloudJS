{
  "extends": [
    "airbnb-base",
    "plugin:node/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "no-bitwise": "off"
  }
}